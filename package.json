  "scripts": {
    "start": "nest start",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"tests/**/*.ts\\\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,tests}/**/*.ts\"",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles --watchAll",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "microorm": "node --require ts-node/register ./node_modules/microorm/cli.js",
    "migration:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
    "app:install": "yarn nestjs-command app:install",
    "app:generate:csv": "yarn nestjs-command app:generate:csv",
    "app:password-reset:clear": "yarn nestjs-command app:password-reset:clear",
    "test:fresh": "NODE_ENV=test mikro-orm migration:fresh",
    "test:seed": "NODE_ENV=test mikro-orm seeder:run --class=TestSeeder"
  },
